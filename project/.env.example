# VoyPath Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Maps API Key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# TravelPayouts API Configuration
VITE_TRAVELPAYOUTS_API_KEY=your_travelpayouts_api_key_here
VITE_TRAVELPAYOUTS_MARKER=your_travelpayouts_marker_here
TRAVELPAYOUTS_TOKEN=your_travelpayouts_token_here

# WayAway Configuration
WAYAWAY_MARKER=649297

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Logging Configuration
# Options: DEBUG, INFO, WARN, ERROR, NONE
# Default: DEBUG in development, WARN in production
VITE_LOG_LEVEL=DEBUG