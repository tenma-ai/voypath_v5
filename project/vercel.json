{
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/wayaway-search",
      "dest": "/api/wayaway-search.js"
    },
    {
      "src": "/api/travelpayouts-proxy",
      "dest": "/api/travelpayouts-proxy.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}